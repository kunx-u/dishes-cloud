<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.etoak.mapper.OrderItemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.etoak.entity.OrderItem">
        <id column="id" property="id" />
        <result column="order_no" property="orderNo" />
        <result column="dishes_id" property="dishesId" />
        <result column="dishes_num" property="dishesNum" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, order_no, dishes_id, dishes_num
    </sql>

</mapper>
